<div 
    data-name="@server.Name"
     data-status="@(server.IsOnline ? "online" : "offline")">
    <p>@server.Name is in @server.City that is @(server.IsOnline ? "online" : "offline") </p>
</div>


@code {

    private Server server = new Server{
        Name = "Server one",
        City = "Český Těšín",
    };
}
